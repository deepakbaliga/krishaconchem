<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deepakbaliga.krishnaconchem">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.intent.action.CALL_PRIVILEGED" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".AboutUsActivity" />
        <activity android:name=".ResearchActivity" />
        <activity android:name=".PublicationsActivity" />
        <activity android:name=".MajorUsersActivity" />
        <activity android:name=".TechnicalDatasheetsActivity" />
        <activity android:name=".CategoryActivity" />
        <activity android:name=".ProductActivity"></activity>
    </application>

</manifest>